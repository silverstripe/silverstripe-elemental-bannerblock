{
    "name": "silverstripe/elemental-bannerblock",
    "description": "A banner block for the silverstripe-elemental module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "content blocks"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/vendor-plugin": "^1.0",
        "silverstripe/elemental-fileblock": "^1.0",
        "silverstripe/cms": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\ElementalBannerBlock\\": "src/",
            "SilverStripe\\ElementalBannerBlock\\Tests\\": "tests/"
        }
    }
}
